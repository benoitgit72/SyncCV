{
  "outputDirectory": ".",
  "functions": {
    "api/chat.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/admin_cv",
      "destination": "/admin_cv/login.html"
    },
    {
      "source": "/admin_cv/:path*",
      "destination": "/admin_cv/:path*"
    },
    {
      "source": "/admin_website",
      "destination": "/admin_website/login.html"
    },
    {
      "source": "/admin_website/:path*",
      "destination": "/admin_website/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}